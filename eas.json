{
    "cli": {
        "version": ">= 5.0.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "android": {
                "buildType": "apk"
            }
        },
        "preview-debug": {
            "android": {
                "buildType": "apk"
            },
            "env": {
                "NODE_ENV": "development"
            }
        },
        "production": {
            "android": {
                "buildType": "apk"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
